				"Tags" : [ { "Key" : "Name", "Value" : {"Fn::Join" : [ "", [
						 {"Fn::GetAtt" : [ "Environment", "NamingPrefix"] },
						 "sip1",
						 {"Fn::GetAtt" : [ "Environment", "NamingSuffix"] }
					] ]}
