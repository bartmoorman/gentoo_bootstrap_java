				],
				"SecurityGroupEgress" : [
					{ "IpProtocol" : "-1", "CidrIp" : "0.0.0.0/0" }
				]
			}
		},

		"InboundIncomingSG" : {
			"Type" : "AWS::EC2::SecurityGroup",
			"Properties" : {
				"VpcId" :  { "Fn::GetAtt" : [ "Environment", "VPC"] },
				"GroupDescription" : "Internet traffic permitted to the Inbound server",
				"SecurityGroupIngress" : [
